<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <TextView
            android:layout_width="173dp"
            android:layout_height="79dp"
            android:text="메뉴"
            android:textSize="40dp"
            android:gravity="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintVertical_bias="0.141"
            android:id="@+id/textView"/>
    <Button
            android:text="추가"
            android:textSize="20dp"
            android:layout_width="159dp"
            android:layout_height="83dp" android:id="@+id/goInsert"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.086" app:layout_constraintVertical_bias="0.531"/>
    <Button
            android:text="검색"
            android:textSize="20dp"
            android:layout_width="159dp"
            android:layout_height="83dp" android:id="@+id/goSearch"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.485" app:layout_constraintVertical_bias="0.531"/>
    <Button
            android:text="기간별 검색"
            android:textSize="20dp"
            android:layout_width="159dp"
            android:layout_height="83dp" android:id="@+id/goSearch2"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.877" app:layout_constraintVertical_bias="0.531"/>

</androidx.constraintlayout.widget.ConstraintLayout>