<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:layout_margin="5dp">

    <TextView
            android:text="수입/지출"
            android:gravity="center"
            android:layout_width="78dp"
            android:layout_height="46dp" android:id="@+id/textType"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.031"
            app:layout_constraintVertical_bias="0.428"/>
    <TextView
            android:text="날짜"
            android:layout_width="186dp"
            android:layout_height="19dp" android:id="@+id/textDate"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.267" app:layout_constraintVertical_bias="0.153"/>
    <TextView
            android:text="내용"
            android:layout_width="274dp"
            android:layout_height="33dp" android:id="@+id/textContent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.949" app:layout_constraintVertical_bias="0.513"/>
    <TextView
            android:text="금액"
            android:layout_width="186dp"
            android:layout_height="22dp" android:id="@+id/textPay"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.267" app:layout_constraintVertical_bias="0.8"/>
</androidx.constraintlayout.widget.ConstraintLayout>