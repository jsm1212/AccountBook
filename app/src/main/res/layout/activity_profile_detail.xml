<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ProfileDetailActivity">

    <TextView
            android:text="내역"
            android:textSize="40dp"
            android:gravity="center"
            android:layout_width="183dp"
            android:layout_height="80dp" android:id="@+id/textView6"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.525" app:layout_constraintVertical_bias="0.077"/>
    <TextView
            android:textSize="30dp"
            android:gravity="center"
            android:layout_width="153dp"
            android:layout_height="85dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.164" app:layout_constraintVertical_bias="0.247"
            android:id="@+id/TextView" android:text="용도"/>
    <TextView
            android:textSize="20dp"
            android:gravity="center"
            android:layout_width="285dp"
            android:layout_height="81dp" android:id="@+id/detailContentText"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.81" app:layout_constraintVertical_bias="0.251"/>
    <TextView
            android:textSize="30dp"
            android:gravity="center"
            android:text="날짜"
            android:layout_width="141dp"
            android:layout_height="80dp" android:id="@+id/textView12"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.16" app:layout_constraintVertical_bias="0.397"/>
    <TextView
            android:textSize="20dp"
            android:gravity="center"
            android:layout_width="279dp"
            android:layout_height="80dp" android:id="@+id/detailDateText"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.794" app:layout_constraintVertical_bias="0.4"/>
    <Button
            android:text="메뉴로"
            android:textSize="20dp"
            android:layout_width="118dp"
            android:layout_height="59dp" android:id="@+id/detailGoMenuBtn"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.946" app:layout_constraintVertical_bias="0.032"/>
</androidx.constraintlayout.widget.ConstraintLayout>